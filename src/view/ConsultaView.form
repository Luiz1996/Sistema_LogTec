                    <Property name="text" type="java.lang.String" value="select distinct&#xa;    l.pk1,&#xa;    l.pk2,&#xa;    l.pk3,&#xa;    l.pk4,&#xa;    l.table_name,&#xa;    l.operation,&#xa;    l.transactionid,&#xa;    l.dml,&#xa;    l.user_name,&#xa;    l.client_address,                               &#xa;    l.systime,&#xa;    d.column_name,&#xa;    d.old_value,&#xa;    d.new_value&#xa;from&#xa;    fz_log_operations l&#xa;left join&#xa;    fz_log_data       d on d.idlogoper = l.idlogoper&#xa;where&#xa;    l.pk1 in () AND&#xa;    l.pk2 in () AND&#xa;    --l.pk3 in () AND&#xa;    --l.pk4 in () AND&#xa;    l.table_name = &apos;NOME_TABELA&apos; and&#xa;    --(d.column_name = &apos;CODMARCA&apos; and (d.old_value = &apos;?&apos; or d.new_value = &apos;?&apos;)) and&#xa;    l.systime between &apos;2017-09-01 00:00:00&apos; and current_timestamp&#xa;order by&#xa;    11,1,2,3,4;"/>
                  </Properties>
                </Component>
